#include <iostream>
#include<algorithm>
#include<unordered_map>
using namespace std;


string SCSS(string s1,string s2,int l1,int l2,unordered_map<string,string>&mmap)
{
    if(l1 == 0 && l2 == 0 )
        return "";
    if(l1 == 0){
        string temp = s2.substr(0,l2);
        std::reverse(temp.begin(),temp.end());
        return temp;
    }
    if(l2 == 0){
        string temp = s1.substr(0,l1);
        std::reverse(temp.begin(),temp.end());
        return temp;
    }
    
    string key = l1 + "SCSS" + l2;
    if(mmap.find(key)!= mmap.end()){
        return mmap[key]; 
    }
    
    if(s1.at(l1-1) == s2.at(l2-1)){
        string temp;
        temp += s1.at(l1-1);
        temp += SCSS(s1,s2,l1-1,l2-1,mmap);
        mmap[key] = temp;
        return temp;
    }    
    else{
        string temp1 = s2.at(l2-1) + SCSS(s1,s2,l1,l2-1,mmap);
        string temp2 = s1.at(l1-1) + SCSS(s1,s2,l1-1,l2,mmap);
        if(temp1.length() > temp2.length()){
            mmap[key] = temp2;
            return temp2;
        }
        else{
            mmap[key] = temp1;
            return temp1;
        }
    }
}




int main()
{
    string x = "";
    string y = "";
    unordered_map<string,string>mmap;
    string rev = SCSS(x,y,x.length(),y.length(),mmap);
    reverse(rev.begin(),rev.end());
    cout<<rev;
    return 0;
}
